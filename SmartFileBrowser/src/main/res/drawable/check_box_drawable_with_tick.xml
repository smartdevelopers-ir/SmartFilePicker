<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="NewApi">
    <item
        android:id="@+id/checked"
        android:state_checked="true"
        android:drawable="@drawable/check_box_checked"
        />
    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/chech_box_unchecked"/>
    <transition
        android:fromId="@id/unchecked"
        android:toId="@id/checked"
        android:drawable="@drawable/avd_uncheck_to_check"
        />
    <transition
        android:fromId="@id/checked"
        android:toId="@id/unchecked"
        android:drawable="@drawable/avd_check_to_uncheck"
        />
</animated-selector>